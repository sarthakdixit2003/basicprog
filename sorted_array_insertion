#include<stdio.h>
void main()
{
    int a[6], val, temp;
    printf("Enter 5 elements:");
    for(int i = 0; i < 5; i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i = 0; i < 5; i++)
    {
        printf("%d ", a[i]);
    }
    printf("\n");
    printf("Enter value:");
    scanf("%d", &val);
    for(int i = 5; i >= 0; i--)
    {
        a[i + 1] = a[i];
        if(a[i] <= val)
        {
            a[i + 1] = val;
            break;
        }
    }
    for(int i = 0; i < 6; i++)
    {
        printf("%d ", a[i]);
    }
}
